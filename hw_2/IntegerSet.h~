// Header file for IntegerSets class
#ifndef INTEGERSET_H
#define INTEGERSET_H

class IntegerSet
{
 private:
  int *arr = nullptr;
  void inputSet(int);
public:
  void unionOfsets();
  void intersectionOfsets();
  void insertElement(int);
  void deleteElement(int);
  void printSet(char);
  void isEqualTo();
  void emptySet();
  void validEntry(int);
  IntegerSet();
  IntegerSet(int []);
};

#endif
